{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier","jest"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
"prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "import/no-extraneous-dependencies": [
            "error",
            {"devDependencies": true}
          ]
    }
}
